<div class="sign-in-header">
  <h4><%= image_tag('H.png') %><p>ackathon<p></h4>
  <%= link_to "Sign Up", new_registration_path(resource_name), :class => 'sign-up-link'%>
</div>
<div class="sign-in-main-container">
  <h2>Login</h2>
  <div class="sign-in-form">
  <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
    <div class="field">
      <%= f.label :email, :class => "label" %><br />
      <%= f.email_field :email, autofocus: true, autocomplete: "email", :class => "input" %>
    </div>

    <div class="field">
      <%= f.label :password, :class => "label" %><br />
      <%= f.password_field :password, autocomplete: "current-password", :class => "input" %>
    </div>

    <div class="actions">
      <%= f.submit "Log in" %>
    </div>
  <% end %>
  </div>
</div>
